{
	"name": "portfolio",
	"version": "1.0.0",
	"description": "Axel Gabriel Calle Granda's portfolio",
	"private": true,
	"author": "Axel Gabriel Calle Granda <97carmine@protonmail.com> (https://github.com/97carmine)",
	"license": "MIT",
	"repository": "github:97carmine/portfolio",
	"bugs": {
		"url": "https://github.com/97carmine/portfolio/issues"
	},
	"homepage": "https://github.com/97carmine/portfolio",
	"browserslist": [
		"defaults"
	],
	"packageManager": "yarn@2.4.2",
	"scripts": {
		"extract:es": "formatjs extract './src/**/*.{js,jsx}' --out-file ./src/common/templates/messages/es.json",
		"extract:en": "formatjs extract './src/**/*.{js,jsx}' --out-file ./src/common/templates/messages/en.json",
		"compile:es": "formatjs compile ./src/common/templates/messages/es.json --ast --out-file ./src/common/languages/messages/es.json",
		"compile:en": "formatjs compile ./src/common/templates/messages/en.json --ast --out-file ./src/common/languages/messages/en.json",
		"build:prod": "rm -rf build && webpack --config webpack.prod.js",
		"build:dev": "rm -rf build && webpack --config webpack.dev.js",
		"start:prod": "node build/server.js",
		"start:dev": "nodemon build/server.js"
	},
	"dependencies": {
		"ajv": "8.5.0",
		"body-parser": "1.19.0",
		"bulma": "0.9.2",
		"dotenv": "10.0.0",
		"express": "4.17.1",
		"helmet": "4.6.0",
		"nodemailer": "6.6.1",
		"prop-types": "15.7.2",
		"react": "17.0.2",
		"react-dom": "17.0.2",
		"react-intl": "5.15.7",
		"react-router": "5.2.0",
		"react-router-dom": "5.2.0",
		"react-transition-group": "4.4.2",
		"winston": "3.3.3"
	},
	"devDependencies": {
		"@axe-core/react": "4.2.0",
		"@babel/cli": "7.13.14",
		"@babel/core": "7.13.15",
		"@babel/eslint-parser": "7.13.14",
		"@babel/plugin-transform-runtime": "7.13.15",
		"@babel/preset-env": "7.13.15",
		"@babel/preset-react": "7.13.13",
		"@babel/runtime": "7.13.10",
		"@formatjs/cli": "4.2.6",
		"babel-loader": "8.2.2",
		"babel-plugin-formatjs": "10.2.6",
		"css-loader": "5.2.4",
		"css-minimizer-webpack-plugin": "2.0.0",
		"eslint": "7.24.0",
		"eslint-config-prettier": "8.3.0",
		"eslint-plugin-formatjs": "2.14.5",
		"eslint-plugin-prettier": "3.4.0",
		"eslint-plugin-react": "7.24.0",
		"eslint-webpack-plugin": "2.5.4",
		"favicons": "6.2.2",
		"favicons-webpack-plugin": "5.0.2",
		"html-minifier-terser": "5.1.1",
		"html-webpack-plugin": "5.3.1",
		"mini-css-extract-plugin": "1.6.0",
		"nodemon": "2.0.7",
		"prettier": "2.2.1",
		"sass": "1.32.8",
		"sass-loader": "12.0.0",
		"stylelint": "13.13.1",
		"stylelint-config-prettier": "8.0.2",
		"stylelint-prettier": "1.2.0",
		"stylelint-webpack-plugin": "2.1.1",
		"terser-webpack-plugin": "5.1.3",
		"webpack": "5.31.2",
		"webpack-cli": "4.6.0",
		"webpack-subresource-integrity": "next"
	}
}
